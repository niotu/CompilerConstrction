// Тест доступа к полям базового класса

class Base is
    var value: Integer(10)
    
    this() is
    end
end

class Derived extends Base is
    method useBaseField(): Integer is
        return value
    end
    
    this() is
    end
end

class Main is
    this() is
        var d: Derived()
        var result: Integer(d.useBaseField())
        result.Print()
    end
end
