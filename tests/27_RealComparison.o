class RealComparisons is
    method compareReals(a : Real, b : Real) : Boolean is
        var isLess : Boolean(a.Less(b))
        var isGreater : Boolean(a.Greater(Real(0.0)))
        var isEqual : Boolean(a.Equal(b))
        var isLessEq : Boolean(a.LessEqual(Real(100.0)))
        var isGreaterEq : Boolean(b.GreaterEqual(a))
        
        var result : Boolean(isLess.Or(isGreater))
        result := result.And(isEqual.Not())
        result := result.Or(isLessEq.And(isGreaterEq))
        
        return result
    end
end

class Main is
    this() is
        var res : Boolean()
        var r : RealComparisons()
        res := r.compareReals(100.1,20.2)
        res.Print()
    end
end