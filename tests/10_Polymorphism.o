class Shape is
    method Area() : Real is
        return 0.0
    end
end

class Circle extends Shape is
    this(rad:Real) is
        r := rad
    end
    var r : Real(0)
    method Area() : Real is
        return r.Mult(r).Mult(3.14)
    end
end

class Main is
    this() is
        var s : Shape()
        s := Circle(2.0)
        var a : Real(s.Area())
        a.Print()
    end
end
