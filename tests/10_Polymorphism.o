class Shape is
    method Area() : Real is
        return 0.0
    end
end

class Circle extends Shape is
    var r : Real(1.0)
    method Area() : Real is
        return r.Mult(r).Mult(3.14159)
    end
end

class PolyTest is
    this() is
        var s : Shape
        s := Circle
        var a : Real(s.Area())
    end
end
