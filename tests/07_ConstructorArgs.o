class App is
    var x : Integer()
    var y : Integer(14)
    var z : Real(2.0)
    method getX() : Integer is
        return x
    end
    this(newX:Integer,newY:Integer,newZ:Real) is
        x := newX
        y := newY
        z := newZ
    end
end

class Main is
    method main() is
        var app : App(1,2,3.8)
        app.x.Print()
    end
end