class Factorial is
    method fact(n : Integer) : Integer is
        if n.Less(2) then
            return 1
        else
            return n.Mul(self.Fact(n.Dec()))
        end
    end

    this() is
    end
end

class Main is
    this() is
        var f : Factorial
        var result : Integer
        result := f.fact(5)
    end
end