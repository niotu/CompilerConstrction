class Factorial is
    method fact(n : Integer) : Integer is
        if n.Less(2) then
            return 1
        else
            return n.Mult(this.fact(n.Minus(1)))
        end
    end
end

class Main is
    method main() is
        var f : Factorial()
        var result : Integer()
        result := f.fact(5)
        result.Print()
    end
end