class IntegerArithmetic is
    method calculate(a : Integer, b : Integer) : Integer is
        var sum : Integer(a.Plus(b))
        var diff : Integer(sum.Minus(Integer(10)))
        var product : Integer(diff.Mult(Integer(2)))
        var quotient : Integer(product.Div(Integer(1)))
        var remainder : Integer(quotient.Rem(Integer(6)))
        return remainder
    end
end
    
class Main is
    this() is
        var remainder : Integer()
        var i : IntegerArithmetic()
        remainder := i.calculate(100,20)
        remainder.Print()
    end
end