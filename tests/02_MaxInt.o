class Main is
    this() is
    end

    method Max(arr : Array[Integer]) : Integer is
        var i : Integer(0)
        var max : Integer(arr.get(0))
        while i.Less(arr.Length) loop
            if arr.get(i).Greater(max) then
                max := arr.get(i)
            end
            i := i.Plus(1)
        end
        return max
    end

    method main() is
        var arr : Array[Integer](5)
        arr.set(0, 3)
        arr.set(1, 7)
        arr.set(2, 2)
        arr.set(3, 9)
        arr.set(4, 1)
        var max : Integer(0)
        max := Max(arr)
        max.Print()
    end
end
