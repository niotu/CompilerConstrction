class MaxInt is
    method Max(arr : Array[Integer]) : Integer is
        var i := 0
        var max := arr.get(0)
        while i < arr.Length loop
            if arr.get(i).Greater(max) then
                max := arr.get(i)
            end
            i := i + 1
        end
        return max
    end

    this() is
        // example constructor (not necessary for testing)
    end
end
