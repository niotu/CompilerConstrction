class Calc is
    var current : Real(0)

    method Add(a : Integer) : Calc is
        current := current.Plus(a)
        return this
    end

    method Add(a : Real) : Calc is
        current := current.Plus(a)
        return this
    end
    
    method getResult() : Real is
        return current
    end
end

class Main is
    this() is
        var calc : Calc()
        var result : Real()
        calc := calc.Add(3).Add(5.5).Add(4.5)
        result := calc.getResult()
        result.Print()
    end
end
