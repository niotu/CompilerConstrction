class Calc is
    var current : Real(0)

    method Add(a : Integer) : Calc is
        current := current.Plus(a)
        return this
    end

    method Add(a : Real) : Calc is
        current := current.Plus(a)
        return this
    end
    
    method getResult() : Real is
        return current
    end

    this() is

    end
end

class Main is
    method main() is
        var calc : Calc()
        var result : Real()
        result := calc.Add(5).Add(3).getResult()
    end
end