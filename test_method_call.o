// Тест вызова методов: this.method() vs method()

class Test is
    var value: Integer(10)
    
    method getValue(): Integer is
        return value
    end
    
    method testThisCall(): Integer is
        return this.getValue()
    end
    
    method testDirectCall(): Integer is
        return this.getValue()
    end
    
    this() is
    end
end

class Main is
    this() is
        var t: Test()
        var res1: Integer(t.testThisCall())
        res1.Print()
        
        var res2: Integer(t.testDirectCall())
        res2.Print()
    end
end
